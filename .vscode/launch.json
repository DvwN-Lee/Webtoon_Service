{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Main",
      "request": "launch",
      "mainClass": "com.webtoon.cli.Main",
      "projectName": "webtoon-rental-system",
      "vmArgs": "-Dfile.encoding=UTF-8 -Dconsole.encoding=UTF-8",
      "console": "integratedTerminal",
      "env": {
        "JAVA_TOOL_OPTIONS": "-Dfile.encoding=UTF-8"
      }
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}",
      "vmArgs": "-Dfile.encoding=UTF-8"
    }
  ]
}
